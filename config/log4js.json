 {
  "appenders": [
    {
      "type": "console"
    },
    {
      "type": "clustered", 
      "appenders": [
        {  
          "type": "dateFile",  
          "filename": "info.log",
          "maxLogSize": 10485760,  
          "pattern": "-yyyy-MM-dd", 
          "numBackups": 5  
        },
        {
          "type": "logLevelFilter", 
          "level": "ERROR", 
          "appender": {
            "type": "dateFile",  
            "filename": "error.log",
            "pattern": "-yyyy-MM-dd"
          }
        }
      ]
    }
  ]
}